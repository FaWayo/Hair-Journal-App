tact' component={ContactUs}/> \n            <Route path='/signup' component={Signup}/>\n            <Route path='/hairtips' component={HairTips}/>\n            <Route path='/videos' component={Videos}/> \n            <Route path='/profile' component={Profile}/>\n            <Route path='/sample' component={SamplePost}/>\n            </div>    \n         </Switch>\n         <Footer/>\n    </Router> \n    </Provider>\n    \n  )\n}\n\nexport default App;\n","C:\\Users\\User\\Desktop\\Pre-Mest Project\\frontend\\src\\reportWebVitals.js",[],"C:\\Users\\User\\Desktop\\Pre-Mest Project\\frontend\\src\\Components\\NaviBar.js",["147"],"import React from 'react'\r\nimport {Link} from 'react-router-dom'\r\nimport {Navbar, Button, Form, FormControl, Nav} from 'react-bootstrap'\r\n\r\n\r\nfunction Navibar() {\r\n    return (\r\n        <div>\r\n           <Navbar bg=\"light\" variant=\"light\">\r\n             <Navbar.Brand href=\"/home\">HAIRRR</Navbar.Brand>\r\n               <Nav className=\"mr-auto\">\r\n                 \r\n                 \r\n                 <Nav.Link href='/videos'>Videos</Nav.Link>\r\n                 <Nav.Link href=\"/hairtips\">Hair Tips</Nav.Link>\r\n                 {/* <Nav.Link href='/contact'>Contact Us</Nav.Link> */}\r\n               </Nav>\r\n        <Form inline>\r\n                 <FormControl type=\"text\" placeholder=\"Search\" className=\"mr-sm-2\" />\r\n                 <Button variant=\"outline-primary\">Search</Button>\r\n               </Form>\r\n            </Navbar>\r\n \r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Navibar\r\n","C:\\Users\\User\\Desktop\\Pre-Mest Project\\frontend\\src\\Components\\Sidebar\\Sidebar.js",["148"],"import React from 'react'\r\nimport {SidebarContent} from './SidebarContent'\r\nimport './Sidebar.css'\r\nimport '../../App.css'\r\nimport {Card, Button} from 'react-bootstrap'\r\nimport {Link} from 'react-router-dom'\r\n \r\nfunction Sidebar() {\r\n    return (\r\n        <div >\r\n            {\r