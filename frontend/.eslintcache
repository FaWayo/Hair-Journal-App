 ? \"line-through\" : \"\" }}\r\n    >\r\n      {todo.text}\r\n      <div>\r\n        <Button variant=\"outline-primary\" size=\"sm\" onClick={() => completeTodo(index)}>completed</Button>\r\n        <Button variant=\"outline-primary\" size=\"sm\" onClick={() => removeTodo(index)}>x</Button>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nfunction TodoForm({ addTodo }) {\r\n  const [value, setValue] = React.useState(\"\");\r\n\r\n  const handleSubmit = e => {\r\n    e.preventDefault();\r\n    if (!value) return;\r\n    addTodo(value);\r\n    setValue(\"\");\r\n  };\r\n\r\n  return (\r\n    <form onSubmit={handleSubmit} >\r\n      <input\r\n        type=\"text\"\r\n        className=\"input\"\r\n        value={value}\r\n        onChange={e => setValue(e.target.value)}\r\n      />\r\n      <Button size=\"sm\" onSubmit={handleSubmit}>Add</Button>\r\n    </form>\r\n  );\r\n}\r\n\r\nfunction ToDoo() {\r\n  const [todos, setTodos] = React.useState([\r\n    \r\n    {\r\n      text: \"Night Routine Tomorrow\",\r\n      isCompleted: false\r\n    },\r\n    {\r\n      text: \"Trim Hair Next Week\",\r\n      isCompleted: false\r\n    }\r\n  ]);\r\n\r\n  const addTodo = text => {\r\n    const newTodos = [...todos, { text }];\r\n    setTodos(newTodos);\r\n  };\r\n\r\n  const completeTodo = index => {\r\n    const newTodos = [...todos];\r\n    newTodos[index].isCompleted = true;\r\n    setTodos(newTodos);\r\n  };\r\n\r\n  const removeTodo = index => {\r\n    const newTodos = [...todos];\r\n    newTodos.splice(index, 1);\r\n    setTodos(newTodos);\r\n  };\r\n\r\n  return (\r\n    <div className=\"app\">\r\n      <Card className=\"todo\" style={{ width: '25rem' }}>\r\n        <Card.Title className='head' className='text-center'>TO-DOs!</Card.Title>\r\n      <div className=\"todo-list\">\r\n        <Card.Body>\r\n        {todos.map((todo, index) => (\r\n          <Todo\r\n            key={index}\r\n            index={index}\r\n            todo={todo} completeTodo={completeTodo}\r\n            \r\n            removeTodo={removeTodo}\r\n          />\r\n        ))}\r\n        <TodoForm addTodo={addTodo} />\r\n        </Card.Body>\r\n      </div>\r\n      </Card>\r\n    </div>\r\n  );\r\n}\r\n\r\n\r\n\r\nexport default ToDoo;\r\n","C:\\Users\\User\\Desktop\\Pre-Mest Project\\frontend\\src\\Components\\Footer\\Footer.js",["122"],"import React from 'react'\r\nimport {Card} from 'react-bootstrap'\r\nimport './Footer.css'\r\nimport {Link} from 'react-router-dom'\r\n\r\nfunction Footer() {\r\n    return (\r\n        \r\n        <div className = 'footer'>\r\n            \r\n            <footer>\r\n            \r\n                              <p>Copyright 2021. Fawayo</p>\r\n                              <a href='/contactus'><p>CONTACT US</p></a>\r\n            </footer>\r\n           \r\n            <div className='media-icons'>\r\n            <Link to=\"#\" className=\"fa fa-facebook\"></Link> \r\n                <Link to=\"#\" className=\"fa fa-twitter\"></Link>\r\n                <Link to=\"#\" className=\"fa fa-instagram\"></Link>\r\n            </div>    \r\n        </div>\r\n       \r\n    )\r\n}\r\n\r\nexport default Footer\r\n","C:\\Users\\User\\Desktop\\Pre-Mest Project\\frontend\\src\\Components\\HairTips\\HairTipsContent.js",[],"C:\\Users\\User\\Desktop\\Pre-Mest Project\\frontend\\src\\Components\\Videos\\Videoo.js",[],"C:\\Users\\User\\Desktop\\Pre-Mest Project\\frontend\\src\\Components\\SamplePost\\SamplePost.js",[],{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","replacedBy":"126"},{"ruleId":"127","severity":1,"message":"128","line":2,"column":9,"nodeType":"129","messageId":"130","endLine":2,"endColumn":13},{"ruleId":"131","severity":1,"message":"132","line":23,"column":68,"nodeType":"133","endLine":23,"endColumn":87},{"ruleId":"123","replacedBy":"134"},{"ruleId":"125","replacedBy":"135"},{"ruleId":"127","severity":1,"message":"136","line":3,"column":15,"nodeType":"129","messageId":"130","endLine":3,"endColumn":21},{"ruleId":"131",