ompleted ? \"line-through\" : \"\" }}\r\n    >\r\n      {todo.text}\r\n      <div>\r\n        <Button variant=\"outline-primary\" size=\"sm\" onClick={() => completeTodo(index)}>completed</Button>\r\n        <Button variant=\"outline-primary\" size=\"sm\" onClick={() => removeTodo(index)}>x</Button>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nfunction TodoForm({ addTodo }) {\r\n  const [value, setValue] = React.useState(\"\");\r\n\r\n  const handleSubmit = e => {\r\n    e.preventDefault();\r\n    if (!value) return;\r\n    addTodo(value);\r\n    setValue(\"\");\r\n  };\r\n\r\n  return (\r\n    <form onSubmit={handleSubmit} >\r\n      <input\r\n        type=\"text\"\r\n        className=\"input\"\r\n        value={value}\r\n        onChange={e => setValue(e.target.value)}\r\n      />\r\n      <Button size=\"sm\" onSubmit={handleSubmit}>Add</Button>\r\n    </form>\r\n  );\r\n}\r\n\r\nfunction ToDoo() {\r\n  const [todos, setTodos] = React.useState([\r\n    \r\n    {\r\n      text: \"Night Routine Tomorrow\",\r\n      isCompleted: false\r\n    },\r\n    {\r\n      text: \"Trim Hair Next Week\",\r\n      isCompleted: false\r\n    }\r\n  ]);\r\n\r\n  const addTodo = text => {\r\n    const newTodos = [...todos, { text }];\r\n    setTodos(newTodos);\r\n  };\r\n\r\n  const completeTodo = index => {\r\n    const newTodos = [...todos];\r\n    newTodos[index].isCompleted = true;\r\n    setTodos(newTodos);\r\n  };\r\n\r\n  const removeTodo = index => {\r\n    const newTodos = [...todos];\r\n    newTodos.splice(index, 1);\r\n    setTodos(newTodos);\r\n  };\r\n\r\n  return (\r\n    <div className=\"app\">\r\n      <Card className=\"todo\" style={{ width: '25rem' }}>\r\n        <Card.Title className='head' className='text-center'>TO-DOs!</Card.Title>\r\n      <div className=\"todo-list\">\r\n        <Card.Body>\r\n        {todos.map((todo, index) => (\r\n          <Todo\r\n            key={index}\r\n            index={index}\r\n            todo={todo} completeTodo={completeTodo}\r\n            \r\n            removeTodo={removeTodo}\r\n          />\r\n        ))}\r\n        <TodoForm addTodo={addTodo} />\r\n        </Card.Body>\r\n      </div>\r\n      </Card>\r\n    </div>\r\n  );\r\n}\r\n\r\n\r\n\r\nexport default ToDoo;\r\n","C:\\Users\\User\\Desktop\\Pre-Mest Project\\frontend\\src\\Components\\Footer\\Footer.js",[],"C:\\Users\\User\\Desktop\\Pre-Mest Project\\frontend\\src\\Components\\HairTips\\HairTipsContent.js",[],"C:\\Users\\User\\Desktop\\Pre-Mest Project\\frontend\\src\\Components\\Videos\\Videoo.js",[],"C:\\Users\\User\\Desktop\\Pre-Mest Project\\frontend\\src\\Components\\SamplePost\\SamplePost.js",[],"C:\\Users\\User\\Desktop\\Pre-Mest Project\\frontend\\src\\utils\\setAuthToken.js",[],["123","124"],"C:\\Users\\User\\Desktop\\Pre-Mest Project\\frontend\\src\\Components\\Journal\\JournalList.js",[],"C:\\Users\\User\\Desktop\\Pre-Mest Project\\frontend\\src\\Components\\Journal\\NewJournal.js",["125","126","127","128","129","130"],"import React, {useState} from 'react'\r\nimport { Form, Card, Col, Button} from 'react-bootstrap'\r\n\r\nimport './Notes.css';\r\n// import axios from 'axios'\r\n\r\nfunction Journal({ backhairlength, fronthairlength, currentregimen, date, time }) {\r\n    return(\r\n        <Card> \r\n            <Card.Title><p>Date:{date} <span>Time:{time}</span></p></Card.Title>\r\n            <Card.Body>\r\n                <ul>\r\n                <li>BackHairLength: {backhairlength}</li>\r\n                <li>Front HairLength: {fronthairlength}</li>\r\n                <li>{currentregimen}</li>\r\n                </ul>\r\n            </Card.Body>\r\n        </Card>\r\n        \r\n    )\r\n}\r\n\r\nfunction Notes() {\r\n    // const API_ENDPOINT = process.env.API_ENDPOINT || 'https://localhost://4000/journals'\r\n    const [ fronthairlength, setFronthair ] = useState('')\r\n    const [ backhairlength, setBackhair ] = useState('')\r\n    const [ currentregimen, setCurrentReg] = useState('')\r\n    const [ succ